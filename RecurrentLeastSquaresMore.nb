 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Psi]", "0"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "73a85ed3-e894-4bfc-a96e-efefe9df0681"],
